{
  "name": "media-inspector",
  "version": "0.4.2",
  "description": "Find media files by searching in the metadata",
  "keywords": [
    "filter",
    "find",
    "media",
    "mediainfo",
    "video"
  ],
  "homepage": "https://github.com/rasmuslp/media-inspector#readme",
  "bugs": {
    "url": "https://github.com/rasmuslp/media-inspector/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rasmuslp/media-inspector.git"
  },
  "license": "ISC",
  "author": {
    "name": "Rasmus Ljungmann Pedersen",
    "email": "rasmuslp@gmail.com"
  },
  "bin": {
    "media-inspector": "./bin/run"
  },
  "files": [
    "bin/",
    "dist/",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc --noEmit false --outDir dist/",
    "full-test": "npm run type-check && npm run lint && npm run test",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts --fix --report-unused-disable-directives .",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "rm -f oclif.manifest.json",
    "publish:beta": "np --no-publish --no-release-draft --any-branch",
    "publish:release": "np --no-publish --no-release-draft",
    "start": "node -r ts-node/register ./bin/run",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --project tsconfig.eslint.json",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "1.20.4",
    "@oclif/plugin-autocomplete": "1.3.6",
    "@oclif/plugin-help": "5.1.19",
    "@oclif/plugin-version": "1.1.3",
    "chalk": "^4.1.2",
    "cli-progress": "3.11.2",
    "debug": "^4.3.4",
    "execa": "^5.1.1",
    "json5": "^2.2.1",
    "luxon": "3.1.1",
    "mediainfo-parser": "^1.1.5",
    "mime-types": "^2.1.35",
    "p-limit": "^3.1.0",
    "split2": "4.1.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@rasmuslp/eslint-config": "5.16.0",
    "@types/cli-progress": "3.11.0",
    "@types/debug": "4.1.7",
    "@types/jest": "29.2.3",
    "@types/luxon": "3.1.0",
    "@types/mime-types": "2.1.1",
    "@types/node": "16.18.4",
    "@types/split2": "3.2.1",
    "eslint-plugin-jest": "27.1.6",
    "jest": "29.3.1",
    "jest-junit": "15.0.0",
    "jest-mock-extended": "3.0.1",
    "np": "7.6.2",
    "oclif": "3.3.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "oclif": {
    "bin": "media-inspector",
    "commands": "./src/cli/commands",
    "topics": {
      "cache": {
        "description": "Manage metadata cache for a directory or file"
      }
    },
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  }
}
